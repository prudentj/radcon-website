#content > section {
    margin-bottom: 2rem;
}
#content > section > header {
    font-size: 125%;
    background-color: #e8bf28;
    color: black;
    padding: .2rem 1rem;
    margin-bottom: .2rem;
	font-weight: normal;
}
header {
    font-weight: bold;
}
div.room {
    float: right;
}
header.panelTitle {
    float: left;
}
.duration {
    clear: both;
    font-style: italic;
    float: right;
    margin-left: 2rem;
    margin-bottom: 1rem;
}
p.description {
    clear: left;
}
div.participants {
    font-style: italic;
}
section.time {
    page-break-inside: avoid;
}
section.panel {
    margin-bottom: 1rem;
    padding: .2rem 1rem;
    margin-left : 1rem;
}
section.panel {
    background-color: #ddd;
    color: black;
    page-break-inside: avoid;
}
section.panel:nth-child(2n) {
    background-color: white;
    color: black;
}
table.dates {
    margin: 1rem;
    text-align: left;
    border-collapse: collapse;
}
table.dates > tbody > tr > td,
table.dates > thead > tr > th {
    padding: .1rem 1.5rem;
    margin: 0;
}
table.dates > tbody > tr.current {
    background-color: #ddd;
    color: black;
}

.warning {
    color: #e8bf28;
    background-color: black;
    padding: .1rem .5rem;
    font-size: 115%;
}

img.participantProfilePicture {
    height: 2in;
    float: right;
    margin-bottom: 1rem;
}

.participant {
    clear: both;
}

.hidden {
    display: none;
}
